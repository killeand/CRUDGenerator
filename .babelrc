{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }, "class-properties"],
        ["@babel/plugin-proposal-private-methods", { "loose": true }, "private-methods"],
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-react-jsx"
    ]
}
{
  "name": "crudgenerator",
  "version": "1.0.0",
  "description": "Application to generate SQL code for CRUD operations in a Database",
  "main": "./prod/electron.init.js",
  "scripts": {
    "watch": "parcel watch ./src/main.html --public-url ./ -d ./dev",
    "electron": "parcel build ./src/main.html --public-url ./ -d ./eldev && cp -f ./electron/electron.init.js ./eldev/electron.init.js && electron ./eldev/electron.init.js",
    "build": "parcel build ./src/main.html --public-url ./ -d ./prod && cp -f ./electron/electron.init.js ./prod/electron.init.js && electron-builder -wl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/killeand/CRUDGenerator.git"
  },
  "keywords": [
    "database",
    "crud",
    "react",
    "html5"
  ],
  "author": {
    "name": "Jason Johnston",
    "email": "killean@shaw.ca"
  },
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/killeand/CRUDGenerator/issues"
  },
  "homepage": "https://github.com/killeand/CRUDGenerator",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "parcel-bundler": "^1.12.5"
  },
  "dependencies": {
    "ace-builds": "^1.4.12",
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "react": "^17.0.1",
    "react-ace": "^9.3.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "build": {
    "appId": "com.chosenatom.crudgenerator",
    "productName": "CRUD Generator",
    "directories": {
      "output": "./bin/"
    },
    "win": {
      "target": [
        "nsis",
        "zip",
        "portable",
        "msi"
      ],
      "icon": "./icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "pacman",
        "zip",
        "tar.gz"
      ],
      "icon": "./icon.ico",
      "category": "Utility"
    }
  }
}
